            </class>
        </classes>
    </test>
    <test name="test3">
        <parameter name="email" value="oosifala@seamfix.com"/>
        <parameter name="systemPort" value="8212"/>
        <parameter name="deviceNo" value="3"/>
        <classes>
            <class name="admin.BiometricUpdate">
