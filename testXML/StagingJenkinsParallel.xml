                    <include name="usernamePasswordLogin" />
                    <include name="fingerprintLogin" />
                    <include name="deactivateAccount"/>
                    <include name="forgotPassword" />
                    <include name="validUserIdLogin" />
                    <include name="erorrMessagesBiosmartValidationTest" />
