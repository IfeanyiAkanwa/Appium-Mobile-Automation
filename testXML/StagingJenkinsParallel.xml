                    <include name="navigateToCaptureMenuTest" />
                    <include name="navigateToAddReg" />
                    <include name="MsisdnCategory" />
                    <include name="additionalRegistration" />
                    <include name="additionalRegTypePrivilege" />
                    <include name="ndcRule" />
